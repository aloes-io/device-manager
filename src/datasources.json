{
  "db": {
    "name": "db",
    "connector": "memory",
    "maxDepthOfQuery": 12,
    "maxDepthOfData": 32
  },
  "transient": {
    "name": "transient",
    "connector": "transient"
  },
  "email": {
    "name": "email",
    "connector": "mail",
    "transports": [
      {
        "type": "smtp",
        "host": "",
        "secure": true,
        "port": 465,
        "auth": {
          "user": "",
          "pass": ""
        }
      }
    ]
  },
  "cache": {
    "host": "127.0.0.1",
    "port": 6379,
    "name": "cache",
    "connector": "kv-redis"
  },
  "storage": {
    "name": "storage",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "root": "./storage",
    "nameConflict": "makeUnique",
    "maxFileSize": "10428800"
  }
}
